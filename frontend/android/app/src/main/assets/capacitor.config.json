{
	"appId": "com.example.app",
	"appName": "archlat",
	"webDir": "dist",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0,
			"launchAutoHide": true,
			"backgroundColor": "#ffffff",
			"androidScaleType": "CENTER_CROP",
			"showSpinner": true
		},
		"PrivacyScreen": {
			"enable": true,
			"imageName": "Splashscreen",
			"contentMode": "scaleAspectFit",
			"preventScreenshots": false
		}
	}
}
